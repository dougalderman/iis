{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/server/",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules/*"
  ]
}
